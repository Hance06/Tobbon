[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=muhendis
Group=www-data
WorkingDirectory=/home/muhendis/Tobbon
ExecStart=/home/muhendis/Tobbon/venv/Scripts/gunicorn --access-logfile - --workers 3 --bind unix:/home/muhendis/Tobbon/core.sock core.wsgi:application

[Install]
WantedBy=multi-user.target